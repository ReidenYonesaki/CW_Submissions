//caughtSpeeding
public int caughtSpeeding(int speed, boolean isBirthday) {
  if (isBirthday==true && speed>=66 && speed<=86)
  {
  return 1;
  }
  if (isBirthday==true && speed<66)
  {
  return 0;
  }
  if (speed>=61 && speed<=81)
  {
  return 1;
  }
  if (speed >= 81)
  {
  return 2;
  }
  return 0;
}

//sortaSum 
public int sortaSum(int a, int b) {
  if ((a+b>=10)&&(a+b<=19))
  {
  return 20;
  }
  return a+b;
}
