//Cigar Party
public boolean cigarParty(int cigars, boolean isWeekend) {
  if (isWeekend == true && cigars>=40)
  {
  return true;
  }
  if (cigars >=40 && cigars<=60)
  {
  return true ;
  }
  else return false;
}

//dateParty
public int dateFashion(int you, int date){
  if(you<=2 || date<=2)
  {
  return 0;
  }
  if(you>=8 || date>=8)
  {
  return 2l
  }
  else return 1;
}

